{
  "name": "good-read",
  "displayName": "goodRead",
  "description": "",
  "version": "0.0.2",
  "publisher": "GY",
  "engines": {
    "node": ">=14.0.0",
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.readNovel",
        "title": "goodRead",
        "icon": {
          "light": "./icon.svg",
          "dark": "./icon.svg"
        }
      },
      {
        "command": "extension.readNovel1",
        "title": "goodRead",
        "icon": {
          "light": "./icon.svg",
          "dark": "./icon.svg"
        }
      },
      {
        "command": "extension.showMySidebar",
        "title": "goodRead",
        "icon": {
          "light": "./icon.svg",
          "dark": "./icon.svg"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "my-sidebar",
          "title": "goodRead",
          "icon": "./icon.svg"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "界面",
      "properties": {
        "novelReader.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the Novel Reader view."
        }
      }
    },
    "views": {
      "my-sidebar": [
        {
          "type": "webview",
          "id": "my-sidebar-view",
          "name": "",
          "icon": "./icon.svg"
        }
      ]
    }
  },
  "scripts": {
    "package": "vsce package",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "iconv-lite": "^0.6.3",
    "jschardet": "^3.1.3"
  },
  "icon": "icon.jpg",
  "repository": {
    "type": "git",
    "url": "https://github.com/GY-Jonh/vscode-goodRead.git"
  }
}